{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "Sentry": {
      "Endpoint": "HTDC_EDO_JOBS_SENTRY_ENDPOINT"
    }
  },
  "AllowedHosts": "*",
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "edo",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Identity": {
    "Authority": "authorityUrl",
    "JobsOptions": "edo/jobs",
    "Secret": "serviceWorkerSecret",
    "ClientId": "service_worker"
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Edo": {
    "EdoOptions": "edo",
    "Api": "apiUrl"
  },
  "Completion": {
    "Url": "/en/api/1.0/internal/bookings/capture",
    "ChunkSize": 5
  },
  "Charge": {
    "Url": "/en/api/1.0/internal/bookings/charge",
    "ChunkSize": 5,
    "DaysBeforeDeadline": 1 
  },
  "Cancellation": {
    "Url": "/en/api/1.0/internal/bookings/cancel",
    "ChunkSize": 5
  },
  "Notification": {
    "Url": "/en/api/1.0/internal/bookings/notify",
    "ChunkSize": 5
  }
}
